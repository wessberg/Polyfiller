{
	"name": "@wessberg/polyfiller",
	"version": "0.0.140",
	"description": "Never worry about polyfills again.",
	"files": [
		"dist/**/*.*",
		"polyfill-lib/**/*.*"
	],
	"scripts": {
		"start": "node ./dist/index.js",
		"generate:sandhog": "sandhog all --yes",
		"generate:changelog": "standard-changelog --first-release",
		"generate:all": "npm run generate:sandhog && npm run generate:changelog",
		"clean": "rimraf dist",
		"lint": "tsc --noEmit && eslint \"src/**/*.ts\" --color",
		"prettier": "prettier --write '{src,test,documentation}/**/*.{js,ts,json,html,xml,css,md}'",
		"test": "NODE_ENV=TEST ava",
		"prebuild": "npm run clean",
		"build": "npm run rollup",
		"watch": "npm run rollup -- --watch",
		"rollup": "rollup -c rollup.config.js",
		"scripts:markdown-format-feature-names": "ts-node --project scripts/tsconfig.json scripts/markdown-format-feature-names.ts",
		"preversion": "NODE_ENV=production npm run build",
		"version": "npm run generate:all && git add .",
		"release": "np --no-cleanup --no-yarn",
		"update:check": "npx npm-check-updates -x np --dep dev,prod",
		"update:commit": "npx npm-check-updates -u -x np --dep dev,prod && npm update && npm install"
	},
	"keywords": [
		"polyfills",
		"shim",
		"web",
		"webservice",
		"typescript",
		"polyfill-app",
		"polyfills-as-a-service"
	],
	"devDependencies": {
		"@rollup/plugin-json": "4.1.0",
		"@types/express": "4.17.11",
		"@types/mime": "2.0.3",
		"@types/morgan": "1.9.2",
		"@types/node": "15.6.0",
		"@types/node-fetch": "2.5.10",
		"@types/semver": "7.3.6",
		"@typescript-eslint/eslint-plugin": "4.24.0",
		"@typescript-eslint/parser": "4.24.0",
		"@wessberg/di-compiler": "2.2.1",
		"@wessberg/rollup-plugin-ts": "1.3.14",
		"@wessberg/ts-config": "1.1.0",
		"ava": "3.15.0",
		"eslint": "7.26.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-import": "2.23.3",
		"eslint-plugin-jsdoc": "34.8.2",
		"husky": "6.0.0",
		"node-fetch": "2.6.1",
		"node-ssh": "^11.1.1",
		"np": "^6.5.0",
		"prettier": "2.3.0",
		"pretty-quick": "3.1.0",
		"rimraf": "3.0.2",
		"rollup": "2.48.0",
		"sandhog": "1.0.38",
		"standard-changelog": "2.0.27",
		"ts-node": "9.1.1",
		"typescript": "4.2.4",
		"useragent-generator": "1.1.1-amkt-22079-finish.0"
	},
	"dependencies": {
		"@babel/core": "7.14.3",
		"@babel/plugin-transform-block-scoping": "7.14.2",
		"@formatjs/intl-datetimeformat": "4.1.1",
		"@formatjs/intl-displaynames": "5.1.1",
		"@formatjs/intl-getcanonicallocales": "1.7.0",
		"@formatjs/intl-listformat": "6.2.0",
		"@formatjs/intl-locale": "2.4.27",
		"@formatjs/intl-numberformat": "7.1.1",
		"@formatjs/intl-pluralrules": "4.0.21",
		"@formatjs/intl-relativetimeformat": "9.1.1",
		"@polyfiller/form-data": "0.0.40",
		"@polyfiller/object-fit": "0.0.39",
		"@sentry/node": "6.4.1",
		"@sentry/tracing": "6.4.1",
		"@swc/core": "1.2.58",
		"@webcomponents/custom-elements": "1.4.3",
		"@webcomponents/shadycss": "1.10.2",
		"@webcomponents/shadydom": "1.8.0",
		"@webcomponents/template": "1.4.4",
		"@wessberg/di": "2.0.3",
		"@wessberg/pointer-events": "1.0.9",
		"@wessberg/stringutil": "1.0.19",
		"Base64": "1.1.0",
		"blob-polyfill": "5.0.20210201",
		"browserslist": "4.16.6",
		"browserslist-generator": "1.0.58",
		"chalk": "4.1.1",
		"console-polyfill": "0.3.0",
		"construct-style-sheets-polyfill": "2.4.16",
		"core-js": "3.12.1",
		"esbuild": "0.12.1",
		"events-polyfill": "2.1.2",
		"express": "4.17.1",
		"find-up": "5.0.0",
		"focus-visible": "5.2.0",
		"http-status-codes": "2.1.4",
		"intersection-observer": "0.12.0",
		"mime": "2.5.2",
		"morgan": "1.10.0",
		"mutationobserver-shim": "0.3.7",
		"node.parentelement": "1.0.2",
		"perfnow": "0.2.0",
		"performance-now": "2.1.0",
		"pm2": "4.5.6",
		"polyfill-library": "3.105.0",
		"proxy-polyfill": "0.3.2",
		"regenerator-runtime": "0.13.7",
		"requestanimationframe": "0.0.23",
		"requestidlecallback": "0.3.0",
		"scroll-behavior-polyfill": "2.0.13",
		"semver": "7.3.5",
		"setimmediate": "1.0.5",
		"systemjs": "6.9.0",
		"temp-dir": "2.0.0",
		"toposort": "2.0.2",
		"tslib": "2.2.0",
		"url-polyfill": "1.1.12",
		"web-animations-js": "2.3.2",
		"xhr-polyfill": "0.1.8",
		"zone.js": "0.11.4"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"browser": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typings": "./dist/index.d.ts",
	"es2015": "./dist/index.js",
	"funding": {
		"type": "github",
		"url": "https://github.com/wessberg/polyfiller?sponsor=1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/wessberg/polyfiller.git"
	},
	"bugs": {
		"url": "https://github.com/wessberg/polyfiller/issues"
	},
	"contributors": [
		{
			"name": "Frederik Wessberg",
			"email": "frederikwessberg@hotmail.com",
			"url": "https://github.com/wessberg",
			"imageUrl": "https://avatars2.githubusercontent.com/u/20454213?s=460&v=4",
			"github": "wessberg",
			"role": "Lead Developer",
			"twitter": "FredWessberg"
		}
	],
	"engines": {
		"node": ">=12.0.0"
	},
	"license": "MIT",
	"ava": {
		"files": [
			"test/**/**.test.ts"
		],
		"verbose": true,
		"timeout": "300s",
		"extensions": [
			"ts"
		],
		"environmentVariables": {
			"NODE_OPTIONS": "--max_old_space_size=4096",
			"TESTING": "true"
		},
		"require": [
			"@wessberg/di-compiler/register"
		]
	}
}
